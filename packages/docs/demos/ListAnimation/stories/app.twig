{% set list = ['about', 'team', 'project', 'blog', 'jobs', 'contact'] %}

<div class="bg-[#13005A] text-white min-h-screen font-mono py-10 px-4">
  <ul class="grid grid-cols-1 max-w-sm mx-auto">
    {% for item in list %}
      <li class="link bg-[#03C988] relative rounded hover:z-[1] mb-2 transition-all ease-[cubic-bezier(0.22,1,0.36,1)]">
        <a class="text-[#140f1b] block p-4" href="#"><span>{{ item }}</span></a>
      </li>
    {% endfor %}
  </ul>
</div>

{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "dom",
      "ES2015",
      "ES2016",
      "ES2017",
      "ES2018",
      "ES2019",
      "ES2020"
    ],
    "strict": false,
    "noEmit": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "include": [
    "src/js/**/*.ts"
  ]
}
